from pytube import YouTube

link = str(input("input a link :- "))
youtube_1 = YouTube(link)

#print(youtube_1.title , youTube_1.thumbnail_url)

videoee = youtube_1.streams.all()
vid = list(enumerate(videoee))
for i in vid:
    print(i)

strm = int(input("enter :"))
videoee[strm].download()
print("successfully")
